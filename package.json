    ]
  },
  "nyc": {
    "exclude": [
      "src/index.js",
      "src/utils/index.js"
    ]
  },
  "dependencies": {
    "compression": "^1.7.3",
    "esm": "^3.0.71",
    "express": "^4.16.3",
    "express-rate-limit": "^2.11.0",
    "helmet": "^3.12.1",
    "memory-cache": "^0.2.0",
    "mongoose": "^5.2.3",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "coveralls": "^3.0.2",
    "eslint": "^5.1.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "nodemon": "^1.18.2",
    "nyc": "^12.0.2",
    "supertest": "^3.1.0"
  }
