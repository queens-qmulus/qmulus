{
  "name": "qmulus",
  "version": "0.0.1",
  "description": "Queen's University Open Data API.",
  "main": "index.js",
  "author": "Alex Adusei",
  "license": "MIT",
  "homepage": "https://github.com/queens-qmulus/qmulus#readme",
  "engines": {
    "node": "10.6.x",
    "npm": "6.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/queens-qmulus/qmulus.git"
  },
  "bugs": {
    "url": "https://github.com/queens-qmulus/qmulus/issues"
  },
  "keywords": [
    "qmulus",
    "api",
    "data",
    "express",
    "queens",
    "queen's",
    "queen's university"
  ],
  "scripts": {
    "start": "node -r esm bin/prod",
    "dev": "nodemon -r esm bin/dev",
    "test": "nyc ava --serial --verbose",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint src test bin/*"
  },
  "ava": {
    "require": [
      "esm"
    ],
    "files": [
      "test/*",
      "!test/index.js"
    ]
  },
  "nyc": {
    "exclude": [
      "src/index.js",
      "src/utils/index.js",
      "src/utils/logger.js"
    ]
  },
  "dependencies": {
    "apicache": "^1.5.2",
    "apidoc": "^0.17.7",
    "compression": "^1.7.4",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "express-rate-limit": "^2.14.2",
    "helmet": "^3.21.1",
    "mongodb-memory-server": "^5.2.3",
    "mongoose": "^5.7.1",
    "morgan": "^1.9.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "coveralls": "^3.0.6",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "nodemon": "^1.19.2",
    "nyc": "^14.1.1",
    "supertest": "^3.4.2"
  }
}
