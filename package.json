{
  "name": "qmulus",
  "version": "0.0.1",
  "description": "Queen's Open Data API.",
  "main": "index.js",
  "author": "Alex Adusei",
  "license": "MIT",
  "homepage": "https://github.com/alexadusei/qmulus#readme",
  "engines": {
    "node": "10.6.x",
    "npm": "6.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexadusei/qmulus.git"
  },
  "bugs": {
    "url": "https://github.com/alexadusei/qmulus/issues"
  },
  "keywords": [
    "qmulus",
    "api",
    "data",
    "express",
    "queens",
    "queen's",
    "queen's university"
  ],
  "scripts": {
    "dev": "nodemon -r esm bin/dev",
    "test": "nyc ava --verbose",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
